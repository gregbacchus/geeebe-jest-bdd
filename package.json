  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^24.0.22",
    "jest": "^24.9.0"
  },
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:gregbacchus/geeebe-jest-bdd.git"
  },
  "devDependencies": {
    "@geeebe/common": "^1.0.61",
    "@types/node": "^12.12.6",
    "nodemon": "^1.19.4",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "libOnly": true
}
